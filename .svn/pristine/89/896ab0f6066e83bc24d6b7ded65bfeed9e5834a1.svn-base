<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.dika.dipnis.MainActivity"
    tools:showIn="@layout/activity_main"
    android:background="@drawable/transparent_background_image">

    <Button
        android:id="@+id/btnDogadjaji"
        android:layout_width="290dp"
        android:layout_height="70dp"
        android:text="@string/strBtnDogadjaji"
        android:textColor="@color/white"
        android:textSize="@dimen/t25"
        android:background="@drawable/round_button"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="101dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnInicijative"
        android:layout_width="290dp"
        android:layout_height="70dp"
        android:text="@string/strBtnInicijative"
        android:textColor="@color/white"
        android:textSize="@dimen/t25"
        android:background="@drawable/round_button"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btnDogadjaji"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/btnProblemi"
        android:layout_marginRight="8dp"
        app:layout_constraintVertical_bias="0.503"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnProblemi"
        android:layout_width="290dp"
        android:layout_height="70dp"
        android:text="@string/strBtnProblemi"
        android:textColor="@color/white"
        android:textSize="@dimen/t25"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="101dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <Button
        android:id="@+id/btnPregledDogadjaja"
        android:layout_width="133dp"
        android:layout_height="53dp"
        android:text="@string/strBtnPregledDogInicProb"
        android:textColor="@color/white"
        android:textSize="@dimen/t20"
        android:background="@drawable/round_button"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/btnDogadjaji"
        app:layout_constraintTop_toTopOf="@+id/btnDogadjaji"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnDogadjaji"
        android:layout_marginBottom="8dp"
        android:visibility="gone" />

    <Button
        android:id="@+id/btnDodajDogadjaj"
        android:layout_width="133dp"
        android:layout_height="53dp"
        android:text="@string/strBtnDodajDogInicProb"
        android:textColor="@color/white"
        android:textSize="@dimen/t20"
        android:background="@drawable/round_button"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="@+id/btnDogadjaji"
        app:layout_constraintTop_toTopOf="@+id/btnDogadjaji"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnDogadjaji"
        android:layout_marginBottom="8dp"
        android:visibility="gone" />

    <Button
        android:id="@+id/btnPregledInicijativa"
        android:layout_width="133dp"
        android:layout_height="53dp"
        android:text="@string/strBtnPregledDogInicProb"
        android:textColor="@color/white"
        android:textSize="@dimen/t20"
        android:background="@drawable/round_button"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/btnInicijative"
        app:layout_constraintTop_toTopOf="@+id/btnInicijative"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnInicijative"
        android:layout_marginBottom="8dp"
        android:visibility="gone"/>

    <Button
        android:id="@+id/btnDodajInicijativu"
        android:layout_width="133dp"
        android:layout_height="53dp"
        android:text="@string/strBtnDodajDogInicProb"
        android:textColor="@color/white"
        android:textSize="@dimen/t20"
        android:background="@drawable/round_button"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="@+id/btnInicijative"
        app:layout_constraintTop_toTopOf="@+id/btnInicijative"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnInicijative"
        android:layout_marginBottom="8dp"
        android:visibility="gone" />

    <Button
        android:id="@+id/btnPregledProblema"
        android:layout_width="133dp"
        android:layout_height="53dp"
        android:text="@string/strBtnPregledDogInicProb"
        android:textColor="@color/white"
        android:textSize="@dimen/t20"
        android:background="@drawable/round_button"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/btnProblemi"
        app:layout_constraintTop_toTopOf="@+id/btnProblemi"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnProblemi"
        android:layout_marginBottom="8dp"
        android:visibility="gone" />

    <Button
        android:id="@+id/btnDodajProblem"
        android:layout_width="133dp"
        android:layout_height="53dp"
        android:text="@string/strBtnDodajDogInicProb"
        android:textColor="@color/white"
        android:textSize="@dimen/t20"
        android:background="@drawable/round_button"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="@+id/btnProblemi"
        app:layout_constraintTop_toTopOf="@+id/btnProblemi"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnProblemi"
        android:layout_marginBottom="8dp"
        android:visibility="gone"/>

</android.support.constraint.ConstraintLayout>
